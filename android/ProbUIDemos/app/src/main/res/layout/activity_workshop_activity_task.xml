<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="de.lmu.ifi.medien.probuidemos.developer_workshop.tasks.taskA.WorkshopActivityTaskA">


    <view
        android:id="@+id/probUIContainer"
        class="de.lmu.ifi.medien.probui.gui.ProbUIContainerRelative"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:clipChildren="false">


        <view
            android:id="@+id/button"
            class="de.lmu.ifi.medien.probuidemos.workshop.tasks.taskA.MyProbUIButton"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="100dp"
            android:onClick="onClickButton"
            android:text="Hello World!" />

    </view>

</RelativeLayout>
